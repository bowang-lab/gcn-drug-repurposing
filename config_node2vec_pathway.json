    },
    "diffusion": {
        "diffusion_embs_dir": "results/covid/",
        "eval_diffusion_embs_dir": "results/"
    },
    "node2vec": {
        "emb_file_prefix": "whole_graph_node2vec_pathway_walk",
        "eval_emb_file_prefix": "eval_node2vec_pathway_walk",
        "walk_length": 16,
        "number_walk": 64
    },
    "gcn": {
