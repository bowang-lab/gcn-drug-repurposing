{
    "name": "Drug Repurposing",
    "n_gpu": 1,
    "load_embs": true,
    "method": "node2vec",
    "eval": {
        "graph": "eval.weighted.edgelist"
    },
    "output": {
        "drug_candidates": "drugs_candidataes_path.txt",
        "graph": "whole_graph_perm.weighted.edgelist"
    },
    "topk": 60,
    "covid": {
        "save_dir": "data/covid_to_protein.tsv",
